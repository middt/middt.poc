{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "dapr-actor-server",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/dapr.actor.server/bin/Debug/net8.0/dapr.actor.server.dll",
            "args": [],
            "cwd": "${workspaceFolder}/dapr.actor.server",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "ASPNETCORE_URLS": "http://localhost:4200",
                "DAPR_HTTP_PORT": "42010",
                "DAPR_GRPC_PORT": "42011"
            },
            "preLaunchTask": "dapr-up-dapr-actor-server",
            "postDebugTask": "dapr-down-dapr-actor-server",
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)",
                "uriFormat": "%s/swagger/"
            }
        },
                {
            "name": "dapr-kafka-pubsub",
            "type": "coreclr",
            "request": "launch",
            "program": "${workspaceFolder}/dapr.kafka.pubsub/bin/Debug/net8.0/dapr.kafka.pubsub.dll",
            "args": [],
            "cwd": "${workspaceFolder}/dapr.kafka.pubsub",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "ASPNETCORE_URLS": "http://localhost:4300",
                "DAPR_HTTP_PORT": "43010",
                "DAPR_GRPC_PORT": "43011",
                "BINDING_NAME":"local-kafka",
                "TOPIC_NAME":"topic1"
            },
            "preLaunchTask": "dapr-up-dapr-kafka-pubsub",
            "postDebugTask": "dapr-down-dapr-kafka-pubsub",
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)",
                "uriFormat": "%s/swagger/"
            }
        }
    ],
    "compounds": [
        {
            "name": "Halay Mode",
            "configurations": [

            ],
            "stopAll": true
        }
    ]
}