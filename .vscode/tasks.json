{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build-dapr-actor-server",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/dapr.actor.server/dapr.actor.server.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "appId": "dapr-actor-server",
            "appPort": 4200,
            "httpPort": 42010,
            "grpcPort": 42011,
            "label": "dapr-up-dapr-actor-server",
            "type": "dapr",
            "dependsOn": "build-dapr-actor-server",
            "componentsPath": "dapr/components"
        },
        {
            "appId": "dapr-actor-server",
            "label": "dapr-down-dapr-actor-server",
            "type": "daprd-down"
        },


                {
            "label": "build-dapr-kafka-pubsub",
